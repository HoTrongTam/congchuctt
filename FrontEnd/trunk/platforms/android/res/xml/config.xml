<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="3" id="com.fpt.dichvucong" ios-CFBundleVersion="2" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
