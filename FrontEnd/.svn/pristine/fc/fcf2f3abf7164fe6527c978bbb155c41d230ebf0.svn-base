.progress {height: 6px;border-radius: 10px;background: #989898;box-shadow: none;margin: 12px 0 25px;position: relative;}
.progress-bar {
    border-radius: 10px;
    background:#009866;
    height: 100%;
    color: #fff;
    text-align: center;
    -webkit-box-shadow: inset 0 -1px 0 rgba(0,0,0,.15);
    box-shadow: inset 0 -1px 0 rgba(0,0,0,.15);
    -webkit-transition: width .6s ease;
    -o-transition: width .6s ease;
    transition: width .6s ease;
    position: relative;
    -webkit-animation: animate-positive 2s;
    animation: animate-positive 2s;
}
.progress .progress-bar:after {
    content: "";
    display: inline-block;
    width: 20px;
    height: 20px;
    background: #00A1D8;
    border-radius: 50%;
    position: absolute;
    top: -7px;
    bottom: 0;
    right: -1px;
    z-index: 1;
}
.progress .progress-value {display: block;font-size: 12px;color: #333;position: absolute;top: 15px;right: -5px;}
.per2-1 {position: absolute;top: 10px;font-size: 11px;width: 100%;display: flex;justify-content: space-between;}
.per2-1b,.per2-1c {position: relative;}
.per2-1b:before,.per2-1c:before {content: "";position: absolute;top: -3px;height: 36%;left: 2px;display: block;z-index: -1;width: 0;border-left: 1px solid #444;}
.per2-1 .per2-1c:before {left: inherit;right: 2px;}
