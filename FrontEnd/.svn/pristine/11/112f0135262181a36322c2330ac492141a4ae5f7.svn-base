<!DOCTYPE html>
<ion-view view-title="{{::title}}" class="chuyen-vb" hide-back-button="true">
    <ion-content scroll="false" class="content-type1">
        <div class="fixed-top list-chuyenvb">
            <label class="item item-input item-stacked-label">
                <textarea placeholder="Nội dung xử lý" class="texta2-1"></textarea>
                <span class="input-label" aria-label="Nội dung xử lý">Nội dung xử lý</span>
            </label>
            <div class="wrapper2-4">
                <label class="item item-input item-stacked-label">
                    <input type="text" placeholder="Số ngày xử lý" class="">
                    <span class="input-label">Số ngày xử lý</span>
                </label>
                <label class="item item-input item-stacked-label">
                    <input type="text" placeholder="Hạn xử lý" class="txt2-4">
                    <span class="input-label">Hạn xử lý</span>
                    <span class="icon2-2 ion-android-calendar"></span>
                </label>
            </div>
            <div class="wrapper2-5">
                <span class="lbl2-4c">Thông báo: </span>
                <label class="lbl2-9">
                    <input type="checkbox" class="cbo2-1"> SMS
                </label>
                <label class="lbl2-9">
                    <input type="checkbox" class="cbo2-1"> Email
                </label>
            </div>
            <div class="content">
                <button class="btn1-1 ion-navigate active" ng-class='{active: tabIndex===0}' ng-click='onTabChange(0)'> Cá nhân</button>
                <button class="btn1-1" ng-class='{active: tabIndex===1}' ng-click='onTabChange(1)'>Phòng ban</button>
            </div>
        </div>
    </ion-content>
    <ion-content class="has-header">
        <div class="group2-1">
            <input type="text" class="txt2-2" placeholder="Nhập tìm kiếm">
