                        <div class="lbl2-17b">
                            <input type="checkbox" class="cbo2-1">
                            <span class="lbl2-17c" ng-model="collapsed" ng-click="collapsed=!collapsed">Nguyễn Minh Hải</span>
                        </div>
                        <label class="star2-1">
                            <input type="checkbox" class="star" value="on">
                            <span class="star-icon iconsprite2-star"></span>
                        </label>
                    </div>
                    <div class="lbl2-19" ng-show="collapsed">
