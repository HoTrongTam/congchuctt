.box1-20 {
    background: #fff;
    border: 1px solid #e1e1e1;
    border-style: solid none;
    margin: 20px auto;
}
.box1-18 {
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.lbl1-17 {
    margin: 0;
    padding: 15px 0 15px 15px;
    font-size: 18px;
    /* color: #333; */
    color: #1760d8;
    font-weight: 500;
    line-height: 25px;
}
.line-bottom {
    position: relative;
}
.line-bottom:after {
    content: '';
    display: inline-block;
    position: absolute;
    bottom: 0;
    left: 15px;
    right: 15px;
    border-bottom: 1px solid #eaeaea;
}
.box1-14 {
    padding: 8px 20px 8px 25px;
}
.lbl1-16 {
    margin: 3px auto;
    color: #888;
    line-height: 20px;
}
.lbl1-16b {
    font-weight: 500;
    color: #666;
    margin: 0 2px;
}
.box1-16 {
    width: 100%;
    min-height: 400px;
}
